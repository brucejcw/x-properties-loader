{
  "name": "x-properties-loader",
  "version": "1.0.2",
  "description": "x-properties-loader for webpack based on [properties](https://www.npmjs.com/package/properties)",
  "main": "index.js",
  "scripts": {
    "lint:js": "eslint ./src/ --fix --format ./node_modules/eslint-friendly-formatter",
    "lint:css": "stylelint --allow-empty-input \"src/**/*.scss\" -s scss",
    "test": "npm run lint:css && npm run lint:js",
    "prepublish": "rm -rf dist && babel src --out-dir dist"
  },
  "author": "bruce.jcw@gmail.com",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "loader-utils": "^1.1.0",
    "properties": "^1.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-friendly-formatter": "^3.0.0",
    "stylelint": "^7.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brucejcw/x-properties-loader.git"
  },
  "keywords": [
    "properties",
    "loader"
  ],
  "bugs": {
    "url": "https://github.com/brucejcw/x-properties-loader/issues"
  },
  "homepage": "https://github.com/brucejcw/x-properties-loader#readme"
